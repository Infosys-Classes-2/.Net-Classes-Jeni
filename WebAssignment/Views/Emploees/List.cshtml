@model List<Employees>

<h4>All Employees</h4>
<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Designation</th>
        <th>Level</th>
        <th>Join Date</th>
        <th>Department</th>  
        <th></th>        
    </tr>
    @foreach(Employees employee in Model)
    {
    <tr>
        <td>@employee.FirstName</td>
        <td>@employee.LastName</td>
        <td>@employee.Designation</td>
        <td>@employee.Level</td>
        <td>@employee.JoinDate</td>
        <td>@employee.Department</td>
        <td> 
            <a href="/employees/edit"><i class="bi bi-pencil-square"></i></a>
                <a href="/employees/delete"><i class="bi bi-trash3"></i></i>

        </a>
        </td>
    </tr>
    }

</table>
