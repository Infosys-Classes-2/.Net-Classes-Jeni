<div class="text-center">
    <h1 class="display-4">List of Names:</h1>
     <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">First</th>
                <th scope="col">Last</th>
                <th scope="col">Handle</th>
            </tr>
        </thead>
        @for(int i=1; i<=10;i++)
        {
        <tbody>
            <tr>
                <th scope="row">@i</th>
                <td>Mark</td>
                <td>Otto</td>
                <td>mdo</td>
            </tr>
       </tbody>
    }
    </table>
</div>
